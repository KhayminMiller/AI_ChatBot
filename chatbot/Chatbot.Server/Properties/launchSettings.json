{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "applicationUrl": "http://localhost:5064",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ConnectionStrings__AzureBlobStorage": "UseDevelopmentStorage=true",
        "ConnectionStrings__DefaultConnection": "Data Source=ocrdata.db"
      }
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "applicationUrl": "https://localhost:7064;http://localhost:5064",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ConnectionStrings__AzureBlobStorage": "UseDevelopmentStorage=true",
        "ConnectionStrings__DefaultConnection": "Data Source=ocrdata.db"
      }
    }
  }
}
