{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "inspectUri": "{wsProtocol}://{url.hostname}:{url.port}/_framework/debug/ws-proxy?browser={browserInspectUri}",
      "applicationUrl": "http://localhost:5028",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ConnectionStrings__AzureBlobStorage": "UseDevelopmentStorage=true",
        "ConnectionStrings__DefaultConnection": "Data Source=ocrdata.db",
        "AzureStorage:ContainerName": "faces"
      }

    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "inspectUri": "{wsProtocol}://{url.hostname}:{url.port}/_framework/debug/ws-proxy?browser={browserInspectUri}",
      "applicationUrl": "https://localhost:7178;http://localhost:5028",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ConnectionStrings__AzureBlobStorage": "UseDevelopmentStorage=true",
        "ConnectionStrings__DefaultConnection": "Data Source=ocrdata.db",
        "AzureStorage:ContainerName": "faces"
      }

    }
  }
}
